<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class name="com.txtago.bean.Content" table="TXG_CONTENT">
        <id name="id" column="TXG_CONTENT_ID" type="integer">
            <generator class="increment"/>
        </id>
 	<property name="campaignId" type="integer" column="CAMPAIGN_ID"/>
	<property name="name" type="string" column="NAME"/>
	<property name="contentUrl" type="string" column="CONTENT_URL"/>
	<property name="contentType" type="string" column="CONTENT_TYPE"/>
	<property name="contentText" type="string" column="CONTENT_TEXT"/>
    </class>
</hibernate-mapping>

